constexpr const char* git_branch = "@libClaPP_GIT_BRANCH@";
constexpr const char* config_time = "@project_CONFIG_TIME@";
constexpr const char* build_type = "@CMAKE_BUILD_TYPE@";

}  // namespace build_info

}  // namespace clapp

#endif
